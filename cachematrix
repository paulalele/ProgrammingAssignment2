##function creates a special "matrix" object that can cache its inverse
makeCacheMatrix <- function(x = matrix()) {
m<-TRUE ## argument that this is a matrix
set<-function(y){
  x<<-y
  m<<-TRUE
  }
get<-function()x
setinverse<-function(inverse) m<<-inverse
getinverse<-function() m ##cache the inverse of the matrix
list(set=set, get=get,
     setinverse=setinverse,
     getinverse=getinverse)

cacheSolve <- function(x, ...) { ##function computes the inverse of the special matrix returned by the above function
        ## Return a matrix that is the inverse of 'x'
  m<-x$getinverse()
  if(!is.true(m))){
    message("getting cached data")
    return(m)
  }
data<-x$get()
m<-inverse(data,...)
x$setinverse(m)
m
}
